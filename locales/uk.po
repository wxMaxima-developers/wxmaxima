"результат обробки рівняння у форматі, у якому б його було представлено "
"функцією:\n"
"\n"
"    Values:[\n"
"        /* m=1.2 tons */\n"
"        m=1.2*10^3,\n"
"        /* 100 km/h*/\n"
"        v=100*10^3/(60*60)\n"
"    ];\n"
"    Energy:W=1/2*m*v^2;\n"
"    at(Energy,Values);\n"
"    W_mech:rhs(%);"

