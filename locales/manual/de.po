"f:sin(a*x);\n"
"with_slider_draw(\n"
"    b,makelist(i/2,i,1,10),\n"
"    title=concat(\"a=\",float(b)),\n"
"    grid=true,\n"
"    explicit(\n"
"        subst(a=b,f),\n"
"        x,0,10\n"
"    )\n"
")$\n"

