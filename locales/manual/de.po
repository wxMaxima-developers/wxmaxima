#, fuzzy, no-wrap
#| msgid ""
#| "     wxdraw2d(\n"
#| "        explicit(\n"
#| "            sin(x),\n"
#| "            x,1,10\n"
#| "        )\n"
#| "     ),wxplot_size=[1600,800]$\n"
msgid ""
"wxdraw2d(\n"
"    explicit(\n"
"        sin(x),\n"
"        x,1,10\n"
"    )\n"
"),wxplot_size=[1600,800]$\n"
